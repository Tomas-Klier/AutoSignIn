{
	"manifest_version": 2,
	
	"name": "AutoSignIn",
	"version": "0.0.1",
	"description": "Automatic SignIn",
	"icons": {
		"16": "img/icon_16.png",
		"48": "img/icon_48.png",
		"128": "img/icon_128.png"
	},
	"browser_action": {
		"default_icon": "img/icon_48.png",
		"default_title": "Auto SignIn",
		"default_popup": "popup.html"
	},
	"background": {
		"page": "background.html"
	},
	"permissions": [
		"http://www.miao2015.pw/*",
		"tabs"
	]
}